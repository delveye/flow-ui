{
  "name": "@delveye/flow-ui",
  "version": "0.0.1",
  "description": "Delveye UI Library",
  "author": {
    "name": "Delveye"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/delveye/flow-ui.git"
  },
  "readme": "README.md",
  "homepage": "https://github.com/delveye/flow-ui#readme",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./style.css": "./dist/style.css",
    "./components/*": {
      "types": "./dist/components/*/index.d.ts",
      "import": "./dist/components/*/index.js",
      "require": "./dist/components/*/index.js"
    }
  },
  "sideEffects": [
    "**/*.css"
  ],
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "prebuild": "rimraf dist components && tsc --project tsconfig.build.json && cpy './**/*' '!**/*.stories.*' '!**/*.ts' '!**/*.tsx' '../components' --parents --cwd=src/",
    "build": "./node_modules/.bin/rollup -c --bundleConfigAsCjs",
    "postbuild": "cpy './**/*.d.ts' '../dist' --parents --cwd=components/ && cpy typings.d.ts dist/ && rimraf components",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@delveye/rollup-css-plugin": "./packages/rollup-css-plugin",
    "@eslint/js": "^9.26.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/blocks": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@storybook/react-webpack5": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.15.3",
    "@types/markdown-it": "^14.1.2",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/ui": "^3.1.3",
    "core-js": "^3.42.0",
    "cpy-cli": "^5.0.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.26.0",
    "eslint-plugin-storybook": "^0.12.0",
    "glob": "^11.0.2",
    "globals": "^16.0.0",
    "happy-dom": "^17.4.6",
    "jsdom": "^26.1.0",
    "markdown-it": "^14.1.0",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.5.3",
    "raw-loader": "^4.0.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.40.1",
    "rollup-plugin-clear": "^2.0.7",
    "storybook": "^8.6.12",
    "style-loader": "^4.0.0",
    "typescript": "^5.8.3",
    "url-loader": "^4.1.1",
    "vitest": "^3.1.3",
    "webpack": "^5.99.8",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1"
  },
  "peerDependencies": {
    "@types/react": ">=18.0.0",
    "@types/react-dom": ">=18.0.0",
    "core-js": ">=3.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "dependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@jetbrains/babel-preset-jetbrains": "^2.4.0",
    "@vitest/coverage-v8": "^3.1.3",
    "babel-loader": "^10.0.0",
    "babel-plugin-transform-define": "^2.1.4",
    "browserslist": "^4.24.5",
    "classnames": "^2.5.1",
    "postcss": "^8.5.3",
    "postcss-calc": "^10.1.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-font-family-system-ui": "^5.0.0",
    "postcss-modules-values-replace": "^4.2.0",
    "postcss-preset-env": "^10.1.6"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
