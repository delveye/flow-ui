{
  "name": "@delveye/flow-ui",
  "version": "0.0.1",
  "description": "Delveye UI Library",
  "author": {
    "name": "Delveye"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/delveye/flow-ui.git"
  },
  "readme": "README.md",
  "homepage": "https://github.com/delveye/flow-ui#readme",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css",
    "./components/*": {
      "import": "./dist/components/*/index.js",
      "require": "./dist/components/*/index.js",
      "types": "./dist/components/*/index.d.ts"
    }
  },
  "sideEffects": [
    "**/*.css"
  ],
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "prebuild": "rimraf dist components && tsc --project tsconfig.build.json && cpy './**/*' '!**/*.stories.*' '!**/*.ts' '!**/*.tsx' ../components --parents --cwd=src/",
    "build": "./node_modules/.bin/rollup -c --bundleConfigAsCjs",
    "postbuild": "cpy './**/*.d.ts' ../dist --parents --cwd=components/ && cpy typings.d.ts dist/ && rimraf components"
  },
  "devDependencies": {
    "@delveye/rollup-css-plugin": "./packages/rollup-css-plugin",
    "@eslint/js": "^9.26.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@types/node": "^22.15.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "core-js": "^3.42.0",
    "cpy-cli": "^5.0.0",
    "eslint": "^9.26.0",
    "glob": "^11.0.2",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.40.1",
    "rollup-plugin-clear": "^2.0.7",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@types/react": ">=18.0.0",
    "@types/react-dom": ">=18.0.0",
    "core-js": ">=3.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "dependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@jetbrains/babel-preset-jetbrains": "^2.4.0",
    "babel-loader": "^10.0.0",
    "babel-plugin-transform-define": "^2.1.4",
    "browserslist": "^4.24.5",
    "classnames": "^2.5.1",
    "postcss": "^8.5.3",
    "postcss-calc": "^10.1.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-font-family-system-ui": "^5.0.0",
    "postcss-modules-values-replace": "^4.2.0",
    "postcss-preset-env": "^10.1.6"
  }
}
